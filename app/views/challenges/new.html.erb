<%= form_for @challenge do |f| %>
	<%= f.label :challenge %>
	<%= f.text_field :challenge %><br>
	<%= f.label :challenge_spot %>
	<%= f.text_field :challenge_spot, :value => @spot %><br>
	<%= f.label :score %>
	<%= f.text_field :score %> <br>
	<%= f.label :skater_id %>
	<%= f.text_field :skater_id %><br><br>
	<%= f.submit 'Add challenge' %>
<% end %>